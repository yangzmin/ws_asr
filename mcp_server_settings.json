{
    "mcpServers": {
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "H:/project/go/ws_asr"
            ]
        },
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ]
        },
        "duckduckgo-search": {
            "command": "npx",
            "args": [
                "-y",
                "duckduckgo-mcp-server"
            ]
        }
    }
}